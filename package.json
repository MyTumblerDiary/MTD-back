  "scripts": {
    "pm2": "pm2 start yarn --name mtd -- start:dev",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" ",
    "start": "cross-env NODE_ENV=prod nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
    "start:prod": "npm run build && cross-env NODE_ENV=prod node dist/main",
    "docker-up-server": "docker-compose up -d my-backend",
    "docker-up": "docker-compose up -d",
    "docker-down": "docker-compose down",
    "rm:all": "sudo rm -rf mysql redis",
    "typeorm": "cross-env NODE_ENV=local ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "typeorm migration:generate -d ./config/src/ormconfig.ts",
    "migration:create": "typeorm migration:run -- -d ./src/config/dataSource.ts",
    "migration-run": "typeorm migration:run -- -d ./src/config/data-source.ts",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
